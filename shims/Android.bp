// Camera
cc_library_shared {
    name: "libcamshim",
    proprietary: true,
    defaults: ["hidl_defaults"],
    srcs: [
        "camera.cpp",
    ],
    cflags: [
        "-Werror",
        "-Wextra",
        "-Wall",
    ],
    shared_libs: [
        "libutils",
    ],
}

// Goodix
cc_library_shared {
    name: "fakelogprint",
    srcs: ["fakelogprint.cpp"],
    shared_libs: ["liblog"],
    compile_multilib: "64",
    vendor: true
}

cc_library_shared {
    name: "wfdshim",
    defaults: ["hidl_defaults"],
    srcs: [
        "wfdshim.cpp",
    ],
    cflags: [
        "-Werror",
        "-Wextra",
        "-Wall",
    ],
    shared_libs: [
        "libutils",
    ],
}
